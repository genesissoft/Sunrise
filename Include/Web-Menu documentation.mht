=0A=
// =
-------------------------------------------------------------------------=
-------=0A=
// global collection containing all menus on current page=0A=
var A_MENUS =3D [];=0A=
=0A=
// =
-------------------------------------------------------------------------=
-------=0A=
// menu class=0A=
function menu (a_items, a_tpl) {=0A=
=0A=
	// browser check=0A=
	if (!document.body || !document.body.style)=0A=
		return;=0A=
=0A=
	// store items structure=0A=
	this.a_config =3D a_items;=0A=
=0A=
	// store template structure=0A=
	this.a_tpl =3D a_tpl;=0A=
=0A=
	// get menu id=0A=
	this.n_id =3D A_MENUS.length;=0A=
=0A=
	// declare collections=0A=
	this.a_index =3D [];=0A=
	this.a_children =3D [];=0A=
=0A=
	// assigh methods and event handlers=0A=
	this.expand      =3D menu_expand;=0A=
	this.collapse    =3D menu_collapse;=0A=
=0A=
	this.onclick     =3D menu_onclick;=0A=
	this.onmouseout  =3D menu_onmouseout;=0A=
	this.onmouseover =3D menu_onmouseover;=0A=
	this.onmousedown =3D menu_onmousedown;=0A=
=0A=
	// default level scope description structure =0A=
	this.a_tpl_def =3D {=0A=
		'block_top'  : 16,=0A=
		'block_left' : 16,=0A=
		'top'        : 20,=0A=
		'left'       : 4,=0A=
		'width'      : 120,=0A=
		'height'     : 22,=0A=
		'hide_delay' : 0,=0A=
		'expd_delay' : 0,=0A=
		'css'        : {=0A=
			'inner' : '',=0A=
			'outer' : ''=0A=
		}=0A=
	};=0A=
	=0A=
	// assign methods and properties required to imulate parent item=0A=
	this.getprop =3D function (s_key) {=0A=
		return this.a_tpl_def[s_key];=0A=
	};=0A=
=0A=
	this.o_root =3D this;=0A=
	this.n_depth =3D -1;=0A=
	this.n_x =3D 0;=0A=
	this.n_y =3D 0;=0A=
=0A=
	// 	init items recursively=0A=
	for (n_order =3D 0; n_order < a_items.length; n_order++)=0A=
		new menu_item(this, n_order);=0A=
=0A=
	// register self in global collection=0A=
	A_MENUS[this.n_id] =3D this;=0A=
=0A=
	// make root level visible=0A=
	for (var n_order =3D 0; n_order < this.a_children.length; n_order++)=0A=
		this.a_children[n_order].e_oelement.style.visibility =3D 'visible';=0A=
}=0A=
=0A=
// =
-------------------------------------------------------------------------=
-------=0A=
function menu_collapse (n_id) {=0A=
	// cancel item open delay=0A=
	clearTimeout(this.o_showtimer);=0A=
=0A=
	// by default collapse to root level=0A=
	var n_tolevel =3D (n_id ? this.a_index[n_id].n_depth : 0);=0A=
	=0A=
	// hide all items over the level specified=0A=
	for (n_id =3D 0; n_id < this.a_index.length; n_id++) {=0A=
		var o_curritem =3D this.a_index[n_id];=0A=
		if (o_curritem.n_depth > n_tolevel && o_curritem.b_visible) {=0A=
			o_curritem.e_oelement.style.visibility =3D 'hidden';=0A=
			o_curritem.b_visible =3D false;=0A=
		}=0A=
	}=0A=
=0A=
	// reset current item if mouse has gone out of items=0A=
	if (!n_id)=0A=
		this.o_current =3D null;=0A=
}=0A=
=0A=
// =
-------------------------------------------------------------------------=
-------=0A=
function menu_expand (n_id) {=0A=
=0A=
	// expand only when mouse is over some menu item=0A=
	if (this.o_hidetimer)=0A=
		return;=0A=
